@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>



<h1>Reading Cooking Data @HttpContextAccessor.HttpContext.Request.Cookies["Username"];</h1>
<h1>Reading Cooking Data @HttpContextAccessor.HttpContext.Session.GetString("MyUserName")</h1>
<h1>Session ID: @HttpContextAccessor.HttpContext.Session.Id</h1>